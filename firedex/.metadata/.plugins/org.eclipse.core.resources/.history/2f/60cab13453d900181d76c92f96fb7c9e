package edu.uci.ics.model;

public class ModifiedFiredexSubscription {
	private String topic;
	private int port;
	
	public ModifiedFiredexSubscription(String topic, int port) {
		this.topic = topic;
		this.port = port;
	}
	
	public String getTopic() {
		return (topic);
	}
	
	public int getPort() {
		return (port);
	}
	
}
